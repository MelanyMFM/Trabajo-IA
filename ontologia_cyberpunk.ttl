@prefix ex: <http://example.org/cyberpunk#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:Afinidad a rdfs:Class .

ex:Arma a rdfs:Class .

ex:Build a rdfs:Class .

ex:Categoria a rdfs:Class .

ex:Equipo a rdfs:Class .

ex:Estadistica a rdfs:Class .

ex:EstiloDeJuego a rdfs:Class .

ex:Habilidad a rdfs:Class .

ex:Habilidade a rdfs:Class .

ex:Implante a rdfs:Class .

ex:Jugador a rdfs:Class .

ex:ArmaMelee rdfs:subClassOf ex:Arma .

ex:BuildNetrunner a ex:Build ;
    ex:descripcion "Especialista en hackeo y sigilo" .

ex:HabilidadPasiva rdfs:subClassOf ex:Habilidad .

ex:ImplanteDefensivo rdfs:subClassOf ex:Implante .

ex:ImplanteOfensivo rdfs:subClassOf ex:Implante .

ex:Jugador1 a ex:Jugador ;
    ex:nombre "V"^^xsd:string ;
    ex:usaArma ex:ArmaHackeo ;
    foaf:nick "V_Punk" .

ex:descripcion a rdf:Property ;
    rdfs:domain ex:Build ;
    rdfs:range xsd:string .

ex:nivel a rdf:Property ;
    rdfs:domain ex:Estadistica ;
    rdfs:range xsd:integer .

ex:nombre a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range xsd:string .

ex:perteneceABuild a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range ex:Build .

ex:poseeEstadistica a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range ex:Estadistica .

ex:relacionaCon a rdf:Property ;
    rdfs:domain ex:Build ;
    rdfs:range ex:Categoria .

ex:tieneAfinidad a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range ex:Afinidad .

ex:tieneImplante a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range ex:Implante .

ex:usaEstiloDeJuego a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range ex:EstiloDeJuego .

ex:usaPistola rdfs:subPropertyOf ex:usaArma .

ex:ArmaDistancia rdfs:subClassOf ex:Arma .

ex:ArmaHackeo a ex:ArmaDistancia .

ex:usaArma a rdf:Property ;
    rdfs:domain ex:Jugador ;
    rdfs:range ex:Arma .

